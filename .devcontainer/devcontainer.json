{
  "name": "article6-methodologies",
  "workspaceFolder": "/workspace/article6-methodologies",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "..",
    "args": {
      "NODE_VERSION": "20"
    }
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "ms-vscode.node-debug2",
    "ms-vscode.cpptools"
  ],
  "postCreateCommand": "git lfs install --system && git lfs install",
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.git",
        "dbaeumer.vscode-eslint"
      ]
    }
  }
}