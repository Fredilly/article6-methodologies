    "eval:params:linear:json": "node scripts/baselines/param-extraction-linear-json.js",
    "cli:query": "node bin/mrv-cli.js",
    "server:http": "node bin/http-engine-adapter.js",
    "validate:guardrails": "node scripts/check-methodology-guardrails.js",
    "status:methods": "node scripts/gen-methods-status.mjs",
    "status:sectors": "node scripts/gen-sector-report.mjs",
    "root-cause:index": "node scripts/gen-root-cause-index.mjs",
    "plan:update:ingest": "node scripts/update-ingest-plan.cjs",
    "build:manifest": "node scripts/build-manifest.mjs",
    "discover:unfccc": "node scripts/discover-unfccc.js",
    "ingest:verify": "bash scripts/ingest-verify.sh",
    "ingest:full": "bash scripts/ingest-full.sh",
    "ingest:all": "bash scripts/ingest-full.sh && node scripts/build-manifest.mjs",
    "test:agriculture-gold": "node tests/agriculture-gold-parity.test.js",
    "test:forestry-gold": "node tests/forestry-gold-parity.test.js",
    "test:paths": "bash scripts/check-ingest-paths.sh",
    "sanity": "bash scripts/ingest-sanity.sh",
    "root-cause:open": "less docs/projects/phase-1-ingestion/ROOT_CAUSE.md",
    "root-cause:new": "node scripts/root-cause-new.cjs"
  },
  "dependencies": {
    "fast-deep-equal": "file:vendor/npm/fast-deep-equal-3.1.3.tgz",
    "json-schema-traverse": "file:vendor/npm/json-schema-traverse-1.0.0.tgz",
    "require-from-string": "file:vendor/npm/require-from-string-2.0.2.tgz",
